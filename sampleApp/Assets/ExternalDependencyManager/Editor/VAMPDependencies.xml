<?xml version="1.0" encoding="UTF-8"?>
<dependencies>
    <androidPackages>
        <androidPackage spec="com.google.android.gms:play-services-base:17.6.0"/>
        <androidPackage spec="com.google.android.gms:play-services-ads-identifier:17.6.0"/>
        <androidPackage spec="androidx.annotation:annotation:1.1.0"/>
        <androidPackage spec="androidx.multidex:multidex:2.0.1"/>
        <androidPackage spec="androidx.appcompat:appcompat:1.3.0"/>
        <!--VAMP-->
        <repositories>
            <repository>https://AdGeneration.github.io/VAMP-Android-SDK/repository</repository>
        </repositories>
        <androidPackage spec="jp.supership.vamp:vamp-sdk:4.6.0"/>
        <!--AdMob-->
        <androidPackage spec="jp.supership.vamp:vamp-admob-adapter:+"/>
        <!--FAN-->
        <androidPackage spec="jp.supership.vamp:vamp-fan-adapter:+"/>
        <!--LINEAds-->
        <androidPackage spec="jp.supership.vamp:vamp-lineads-adapter:+"/>
        <!--maio-->
        <androidPackage spec="jp.supership.vamp:vamp-maio-adapter:+">
            <repositories>
                <repository>https://imobile-maio.github.io/maven</repository>
            </repositories>
        </androidPackage>
        <!--nend-->
        <androidPackage spec="jp.supership.vamp:vamp-nend-adapter:+">
            <repositories>
                <repository>https://fan-adn.github.io/nendSDK-Android-lib/library</repository>
            </repositories>
        </androidPackage>
        <!--Pangle-->
        <androidPackage spec="jp.supership.vamp:vamp-pangle-adapter:+">
            <repositories>
                <repository>https://artifact.bytedance.com/repository/pangle</repository>
            </repositories>
        </androidPackage>
        <!--Tapjoy-->
        <androidPackage spec="jp.supership.vamp:vamp-tapjoy-adapter:+">
            <repositories>
                <repository>https://sdk.tapjoy.com/</repository>
            </repositories>
        </androidPackage>
        <!--UnityAds-->
        <androidPackage spec="jp.supership.vamp:vamp-unityads-adapter:+"/>
    </androidPackages>
    <iosPods>
        <!--VAMP-->
        <iosPod name="VAMP-SDK" version="4.6.0"/>
        <!--AdMob-->
        <iosPod name="VAMPAdmobAdapter"/>
        <!--FAN-->
        <iosPod name="VAMPFANAdapter"/>
        <!--LINEAds-->
        <iosPod name="VAMPLINEAdsAdapter"/>
        <!--maio-->
        <iosPod name="VAMPMaioAdapter"/>
        <!--nend-->
        <iosPod name="VAMPNendAdapter"/>
        <!--Pangle-->
        <iosPod name="VAMPPangleAdapter"/>
        <!--Tapjoy-->
        <iosPod name="VAMPTapjoyAdapter"/>
        <!--UnityAds-->
        <iosPod name="VAMPUnityAdsAdapter"/>
    </iosPods>
</dependencies>
